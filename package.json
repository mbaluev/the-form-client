    "prettier": "prettier --parser typescript --single-quote",
    "prettier:check": "npm run prettier -- --list-different \"src/**/*.{ts,tsx}\"",
    "prettier:write": "npm run prettier -- --write \"src/**/*.{ts,tsx}\"",
    "test": "jest",
    "test:ci": "jest --ci",
    "test:watch": "jest --watch",
    "prepare": "husky install",
    "docker": "docker build -t the-form-client . && docker run --env-file .env.production -dp 127.0.0.1:80:80 the-form-client"
